<!doctype html>
<html lang="en">
  <head>
    {{ content_for_header }}
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="https://cycycy.com" rel="canonical">
    <title>CY</title>
    <meta
      name="description"
      content="CY is a cross between a gallery, a shop and atelier dedicated to sourcing and curating design and art. 7296 NW 1st Ct, Miami FL 33150"
    >
    <meta content="index,follow,noodp" name="robots">
    <meta content="CY" property="og:title">
    <meta content="website" property="og:type">
    <meta content="https://cycycy.com" property="og:url">
    <meta
      content="CY is a cross between a gallery, a shop and atelier dedicated to sourcing and curating design and art. 7296 NW 1st Ct, Miami FL 33150"
      itemprop="description"
    >
    {%- if settings.favicon != blank -%}
      <link rel="icon" type="image/png" href="{{ settings.favicon | image_url: width: 32, height: 32 }}">
    {%- endif -%}
    {% comment %} <meta content="https://cycycy.com/assets/images/share.png" property="og:image"> {% endcomment %}
    <meta content="1200" property="og:image:width">
    <meta content="630" property="og:image:height">
    <script type="application/ld+json">
      {
        "@context": "https://schema.org",
        "@graph": [
          {
            "@type": "WebSite",
            "@id": "https://cycycy.com/#website",
            "url": "https://cycycy.com",
            "name": "CY",
            "description": "CY is a cross between a gallery, a shop and atelier dedicated to sourcing and curating design and art. 7296 NW 1st Ct, Miami FL 33150"
          }
        ]
      }
    </script>

    <script>
      /// VH
      let vh = window.innerHeight * 0.01;
      document.documentElement.style.setProperty('--vh', `${vh}px`);
      window.addEventListener('resize', () => {
        let vh = window.innerHeight * 0.01;
        document.documentElement.style.setProperty('--vh', `${vh}px`);
      });
    </script>
    <style data-shopify>
      @font-face {
        font-family: 'Editorial New';
        font-style: normal;
        font-weight: 200;
        font-display: swap;
        src: local(''),
          url(https://cycycy.com/cdn/shop/t/5/assets/PPEditorialNew-Ultralight.woff2?v=105689704511949219151708049829)
            format('woff2'),
          url(https://cycycy.com/cdn/shop/t/5/assets/PPEditorialNew-Ultralight.woff?v=123256282314567908221708049738)
            format('woff'),
          url(https://cycycy.com/cdn/shop/t/5/assets/PPEditorialNew-Ultralight.ttf?v=66626499898420067261708049738)
            format('truetype');
      }

      :root {
        --body-font: 'Editorial New', serif;
      }
    </style>
    <style type="text/css">
      .hidden {
        opacity: 0;
      }

      .visible {
        opacity: 1;
        transition: opacity 1s ease-in-out;
      }
      :root {
        --vh: ;
      }
      * {
        box-sizing: border-box;
      }
      html,
      body {
        margin: 0;
        padding: 0;
        border: 0;
        font: inherit;
        vertical-align: baseline;
        word-break: break-word;
      }
      body {
        font-family: 'Editorial New', serif;
        -webkit-font-smoothing: antialiased;
        letter-spacing: 2.25px !important;
        margin: 0 auto;
        padding: 4.8rem 2rem 3.3rem 2rem;
        display: flex;
        flex-direction: column;
        text-align: center;
        align-items: center;
        justify-content: space-between;
        font-size: 11.25px;
        height: calc(var(--vh, 1vh) * 100);
        color: #010101 !important;
        background-color: #fff;
        -webkit-user-select: none; /* Safari */
        -ms-user-select: none; /* IE 10 and IE 11 */
        user-select: none; /* Standard syntax */
        opacity: 1;
      }
      h1 {
        margin-block-start: 0em;
        margin-block-end: 0em;
        font-weight: normal;
        font-size: inherit;
      }
      a {
        text-decoration: none;
        color: inherit;
        outline: none;
        padding: 2px 4px;
      }
      a:hover {
        outline: none;
      }
      a:focus,
      button:focus {
        outline: none;
      }
      a:focus-visible {
        outline: 1px solid #010101;
        outline-offset: 2px;
      }
      a:focus-visible {
        background: none;
      }
      *:focus {
        outline: none;
      }
      .visually-hidden {
        border: 0;
        padding: 0;
        margin: 0;
        position: absolute !important;
        height: 1px;
        width: 1px;
        overflow: hidden;
        clip: rect(1px 1px 1px 1px);
        clip: rect(1px, 1px, 1px, 1px);
        clip-path: inset(50%);
        white-space: nowrap;
      }
      .uppercase {
        text-transform: uppercase;
      }
      .stacked {
        margin-bottom: 2.4rem;
      }
      .stacked a {
        display: block;
      }
      address {
        font-style: normal;
      }
      .logo {
        height: 69px;
        width: auto;
      }
      @media screen and (min-width: 728px) {
        body {
          padding: 5.8rem 2rem 4.1rem 2rem;
        }
        .stacked {
          margin-bottom: 2.9rem;
        }
      }
      #WebPixelsManagerSandboxContainer {
        display: none;
      }
      form {
        display: flex;
        flex-direction: column;
        gap: 0.5rem;
      }
      input {
        font-family: 'Editorial New', serif;
        -webkit-appearance: none;
        -moz-appearance: none;
        appearance: none;
        border: none;
        border-radius: 0;
        outline: none;
        padding: 0.2rem;
        font-size: 0.8rem;
        letter-spacing: 0.1em;
        text-align: center;
      }
      input::placeholder {
        font-family: 'Editorial New', serif;
        letter-spacing: 0.1em;
        text-align: center;
      }
      button {
        font-family: 'Editorial New', serif;
        border: none;
        border-radius: 0;
        outline: none;
        padding: 0.2rem;
        background: white;
        font-size: 0.8rem;
        letter-spacing: 0.1em;
        text-align: center;
        color: black;
      }
      button:hover {
        cursor: pointer;
      }
    </style>
  </head>

  <body class="hidden">
    <header>
      <h1>
        <span class="visually-hidden">CY</span>
        <svg
          class="logo"
          id="Layer_2"
          data-name="Layer 2"
          xmlns="http://www.w3.org/2000/svg"
          viewBox="0 0 29.42 107.82"
        >
          <g id="Capa_1" data-name="Capa 1">
            <g>
              <path fill="#010101" d="m3.52,9.83c1.16-3.03,2.82-5.43,4.98-7.19C10.66.88,13.15,0,15.97,0c1.43,0,2.65.19,3.67.57,1.02.38,2.03.89,3.05,1.52.73.48,1.25.71,1.57.71s.54-.14.67-.43.19-.92.19-1.9h1l.71,13.09h-1c-.41-4.03-1.42-7.03-3.02-9-1.6-1.97-3.75-2.95-6.45-2.95-2.32,0-4.33.81-6.05,2.43-1.71,1.62-3.03,3.83-3.95,6.62-.92,2.79-1.38,5.9-1.38,9.33s.46,6.53,1.38,9.31c.92,2.78,2.23,4.97,3.93,6.57,1.7,1.6,3.71,2.4,6.02,2.4,2.98,0,5.47-1.32,7.45-3.98,1.98-2.65,3.24-6.2,3.78-10.64h1c-.6,5.02-1.98,8.99-4.12,11.93-2.14,2.94-5.04,4.4-8.69,4.4-2.83,0-5.29-.88-7.4-2.64-2.11-1.76-3.73-4.16-4.86-7.19-1.13-3.03-1.69-6.42-1.69-10.16s.58-7.13,1.74-10.16Z"/>
              <path fill="#010101" d="m16.97,105.53c.21.48.64.81,1.31,1,.67.19,1.78.29,3.33.29v1h-12.85v-1c1.56,0,2.67-.1,3.33-.29.67-.19,1.1-.52,1.31-1,.21-.48.31-1.24.31-2.29v-12.19l-8.57-17.24c-.64-1.3-1.29-2.14-1.98-2.52-.68-.38-1.74-.57-3.17-.57v-1h11.67v1c-1.46,0-2.46.06-3,.19-.54.13-.81.38-.81.76,0,.25.12.63.36,1.12s.4.83.5,1.02l7.24,14.66h.14l6.57-14.62c.48-1.05.71-1.75.71-2.1,0-.41-.26-.69-.79-.83-.52-.14-1.53-.21-3.02-.21v-1h9.86v1c-1.43,0-2.47.2-3.12.6-.65.4-1.28,1.25-1.88,2.55l-7.76,17.19v12.19c0,1.05.1,1.81.31,2.29Z"/>
            </g>
          </g>
        </svg>
      </h1>
    </header>

    <div>
      <address>
        <a target="_blank" href="https://goo.gl/maps/VaHvbALAgc6hMU3NA">7296 NW 1st Ct, Miami FL 33150</a>
      </address>
    </div>

    <div class="uppercase">
      <address>
        <div class="stacked">
          <a target="_blank" href="https://www.instagram.com/cycycy_______">Instagram</a>
          <a href="mailto:cy@cycycy.com">E-mail</a>
          <!-- <a target="_blank" href="tel:111111111">Call</a> -->
          <a href="https://cycycy.com">cycycy.com</a>
        </div>
        <div>
          {{ content_for_layout }}
          {%- form 'storefront_password', class: 'password-form' -%}
            <div class="password-field field{% if form.errors %} password-field--error{% endif %}">
              <input
                type="password"
                name="password"
                id="Password"
                class="field__input"
                autocomplete="current-password"
                {% if form.errors %}
                  aria-invalid="true"
                  aria-describedby="PasswordLoginForm-password-error"
                {%- endif -%}
                placeholder="{{ 'general.password_page.login_form_password_placeholder' | t }}"
              >
              {% comment %}
                <label class="field__label" for="{{ 'general.password_page.login_form_password_label' | t }}">
                  {{- 'general.password_page.login_form_password_placeholder' | t -}}
                </label>
              {% endcomment %}
              {%- if form.errors -%}
                <div id="PasswordLoginForm-password-error" role="status">
                  <span class="visually-hidden">{{ 'accessibility.error' | t }}</span>
                  <span class="form__message">
                    {%- render 'icon-error' %}
                    {{ 'general.password_page.login_form_error' | t -}}
                  </span>
                </div>
              {%- endif -%}
            </div>
            <button name="commit" class="password-button button button--outline">
              {{ 'general.password_page.login_form_submit' | t }}
            </button>
          {%- endform -%}
        </div>
      </address>
    </div>
  </body>

  <script>
    window.onload = (event) => {
      document.body.className = 'visible';
    };
  </script>
</html>
